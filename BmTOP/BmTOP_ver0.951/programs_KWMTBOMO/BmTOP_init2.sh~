#!/bin/sh
# 最初に下記のパスを書き換えること!!
#   パスは、絶対パスでないとダメ
#   *相対パスだとシンボリックリンクからアクセスできない

#gnome-terminal -e "sh -c 'python3 /home/mizuki/Documents/Atom_workspace/BmTOP/programs_KWMTBOMO/BmTOP_path.py;exec bash'"

export PWD = `pwd`


gnome-terminal -e "sh -c 'python3 PWD/BmTOP_path.py;exec bash'"

